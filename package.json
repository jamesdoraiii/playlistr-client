{
  "name": "playlistr-client",
  "version": "0.0.1",
  "scripts": {
    "analyze": "webpack-bundle-analyzer dist/meraki-pipeline/stats-es2015.json",
    "ng": "ng",
    "start": "npm run tailwind && ng serve -o",
    "build": "npm run tailwind && npm run purge && ng build",
    "build:stats": "npm run tailwind && npm run purge && ng build --stats-json",
    "build:analyze": "npm run build:stats && npm run analyze",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "purge": "purgecss -c ./purgecss.config.js -o src/",
    "tailwind": "./node_modules/.bin/tailwind build ./src/tailwind-build.css -c ./tailwind.config.js -o ./src/styles.tailwind.css"
  },
  "dependencies": {
    "@angular/animations": "^10.1.3",
    "@angular/common": "^10.1.3",
    "@angular/compiler": "^10.1.3",
    "@angular/core": "^10.1.3",
    "@angular/forms": "^10.1.3",
    "@angular/localize": "^10.1.3",
    "@angular/platform-browser": "^10.1.3",
    "@angular/platform-browser-dynamic": "^10.1.3",
    "@angular/router": "^10.1.3",
    "@apollo/client": "^3.0.0",
    "@types/spotify-web-playback-sdk": "^0.1.7",
    "apollo-angular": "^2.0.4",
    "apollo-angular-link-http": "^1.11.0",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "bcrypt": "^5.0.0",
    "core-js": "^3.2.1",
    "graphql": "^15.0.0",
    "ng-packagr": "^10.1.0",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "webpack-bundle-analyzer": "^3.4.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.3",
    "@angular/cli": "~10.1.3",
    "@angular/compiler-cli": "^10.1.3",
    "@angular/language-service": "^10.1.3",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.19.15",
    "codelyzer": "^5.1.2",
    "husky": "^3.0.3",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "protractor": "~7.0.0",
    "purgecss": "^1.3.0",
    "tailwindcss": "^1.7.6",
    "tailwindcss-debug-screens": "^2.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
